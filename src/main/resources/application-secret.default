      accessKey: #AWS BUCKET ACCESS-KEY
      secretKey: #AWS BUCKET SECRET-KEY
      region: #REGION
      endPoint: #STORAGE ENDPOINT
      bucket: #BUCKET NAME
      imgExt: #이미지 파일 확장자 리스트
      cdnUrl: #CDN URL
    gcp:
      auth:
        filePath: #GCP AUTH FILEPATH